{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": ["nfInstanceId", "nfType", "nfStatus"],
    "properties": {
        "nfInstanceId": {
            "type": "string"
        },
        "nfType": {
            "$ref": "#/components/schemas/NFType"
        },
        "nfStatus": {
            "$ref": "#/components/schemas/NFStatus"
        },
        "heartBeatTimer": {
            "type": "integer"
        },
        "plmnList": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/PlmnId"
            },
            "minItems": 1
        },
        "sNssais": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/Snssai"
            },
            "minItems": 1
        },
        "nsiList": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1
        },
        "fqdn": {
            "$ref": "#/components/schemas/Fqdn"
        },
        "interPlmnFqdn": {
            "$ref": "#/components/schemas/Fqdn"
        },
        "ipv4Addresses": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/Ipv4Addr"
            },
            "minItems": 1
        },
        "ipv6Addresses": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/Ipv6Addr"
            },
            "minItems": 1
        },
        "allowedPlmns": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/PlmnId"
            },
            "minItems": 1
        },
        "allowedNfTypes": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/NFType"
            },
            "minItems": 1
        },
        "allowedNfDomains": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1
        },
        "allowedNssais": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/Snssai"
            },
            "minItems": 1
        },
        "priority": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535
        },
        "capacity": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535
        },
        "load": {
            "type": "integer",
            "minimum": 0,
            "maximum": 100
        },
        "locality": {
            "type": "string"
        },
        "udrInfo": {
            "$ref": "#/components/schemas/UdrInfo"
        },
        "udmInfo": {
            "$ref": "#/components/schemas/UdmInfo"
        },
        "ausfInfo": {
            "$ref": "#/components/schemas/AusfInfo"
        },
        "amfInfo": {
            "$ref": "#/components/schemas/AmfInfo"
        },
        "smfInfo": {
            "$ref": "#/components/schemas/SmfInfo"
        },
        "upfInfo": {
            "$ref": "#/components/schemas/UpfInfo"
        },
        "pcfInfo": {
            "$ref": "#/components/schemas/PcfInfo"
        },
        "bsfInfo": {
            "$ref": "#/components/schemas/BsfInfo"
        },
        "chfInfo": {
            "$ref": "#/components/schemas/ChfInfo"
        },
        "nrfInfo": {
            "$ref": "#/components/schemas/NrfInfo"
        },
        "customInfo": {
            "$ref": "#/components/schemas/CustomInfo"
        },
        "recoveryTime": {
            "$ref": "#/components/schemas/DateTime"
        },
        "nfServicePersistence": {
            "type": "boolean"
        },
        "nfServices": {
            "type": "array",
            "items": {
                "$ref": "#/components/schemas/NFService"
            },
            "minItems": 1
        },
        "provisionInfo": {
            "$ref": "#/components/schemas/ProvisionInfo"
        }
    },
    "components": {
        "schemas": {
            "NFType": {
                "anyOf": [{
                        "type": "string",
                        "enum": [
                            "NRF",
                            "UDM",
                            "AMF",
                            "SMF",
                            "AUSF",
                            "NEF",
                            "PCF",
                            "SMSF",
                            "NSSF",
                            "UDR",
                            "LMF",
                            "GMLC",
                            "5G_EIR",
                            "SEPP",
                            "UPF",
                            "N3IWF",
                            "AF",
                            "UDSF",
                            "BSF",
                            "CHF",
                            "NWDAF"
                        ]
                    },
                    {
                        "type": "string"
                    }
                ]
            },
            "NFStatus": {
                "anyOf": [{
                        "type": "string",
                        "enum": ["REGISTERED", "SUSPENDED"]
                    },
                    {
                        "type": "string"
                    }
                ]
            },
            "PlmnId": {
                "type": "object",
                "additionalProperties": false,
                "required": ["mcc", "mnc"],
                "properties": {
                    "mcc": {
                        "$ref": "#/components/schemas/Mcc"
                    },
                    "mnc": {
                        "$ref": "#/components/schemas/Mnc"
                    }
                }
            },
            "Mcc": {
                "type": "string",
                "pattern": "^\\d{3}$"
            },
            "Mnc": {
                "type": "string",
                "pattern": "^\\d{2,3}$"
            },
            "Snssai": {
                "type": "object",
                "additionalProperties": false,
                "required": ["sst"],
                "properties": {
                    "sst": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "sd": {
                        "type": "string",
                        "pattern": "^[A-Fa-f0-9]{6}$"
                    }
                }
            },
            "Fqdn": {
                "type": "string"
            },
            "Ipv4Addr": {
                "type": "string",
                "pattern": "^(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])$"
            },
            "Ipv6Addr": {
                "type": "string",
                "allOf": [{
                        "pattern": "^((:|(0?|([1-9a-f][0-9a-f]{0,3}))):)((0?|([1-9a-f][0-9a-f]{0,3})):){0,6}(:|(0?|([1-9a-f][0-9a-f]{0,3})))$"
                    },
                    {
                        "pattern": "^((([^:]+:){7}([^:]+))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?))$"
                    }
                ]
            },
            "Ipv6Prefix": {
                "type": "string",
                "allOf": [{
                        "pattern": "^((:|(0?|([1-9a-f][0-9a-f]{0,3}))):)((0?|([1-9a-f][0-9a-f]{0,3})):){0,6}(:|(0?|([1-9a-f][0-9a-f]{0,3})))(\\/(([0-9])|([0-9]{2})|(1[0-1][0-9])|(12[0-8])))$"
                    },
                    {
                        "pattern": "^((([^:]+:){7}([^:]+))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?))(\\/.+)$"
                    }
                ]
            },
            "UdrInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "groupId": {
                        "$ref": "#/components/schemas/NfGroupId"
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        },
                        "minItems": 1
                    },
                    "gpsiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        },
                        "minItems": 1
                    },
                    "externalGroupIdentifiersRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        },
                        "minItems": 1
                    },
                    "supportedDataSets": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DataSetId"
                        },
                        "minItems": 1
                    }
                }
            },
            "SupiRange": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "start": {
                        "type": "string",
                        "pattern": "^[0-9]+$"
                    },
                    "end": {
                        "type": "string",
                        "pattern": "^[0-9]+$"
                    },
                    "pattern": {
                        "type": "string"
                    }
                }
            },
            "IdentityRange": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "start": {
                        "type": "string",
                        "pattern": "^[0-9]+$"
                    },
                    "end": {
                        "type": "string",
                        "pattern": "^[0-9]+$"
                    },
                    "pattern": {
                        "type": "string"
                    }
                }
            },
            "DataSetId": {
                "anyOf": [{
                        "type": "string",
                        "enum": ["SUBSCRIPTION", "POLICY", "EXPOSURE", "APPLICATION"]
                    },
                    {
                        "type": "string"
                    }
                ]
            },
            "UdmInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "groupId": {
                        "$ref": "#/components/schemas/NfGroupId"
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        },
                        "minItems": 1
                    },
                    "gpsiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        },
                        "minItems": 1
                    },
                    "externalGroupIdentifiersRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        },
                        "minItems": 1
                    },
                    "routingIndicators": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^[0-9]{1,4}$"
                        },
                        "minItems": 1
                    }
                }
            },
            "AusfInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "groupId": {
                        "$ref": "#/components/schemas/NfGroupId"
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        },
                        "minItems": 1
                    },
                    "routingIndicators": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^[0-9]{1,4}$"
                        },
                        "minItems": 1
                    }
                }
            },
            "AmfInfo": {
                "type": "object",
                "additionalProperties": false,
                "required": ["amfSetId", "amfRegionId", "guamiList"],
                "properties": {
                    "amfSetId": {
                        "type": "string"
                    },
                    "amfRegionId": {
                        "type": "string"
                    },
                    "guamiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Guami"
                        },
                        "minItems": 1
                    },
                    "taiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Tai"
                        },
                        "minItems": 1
                    },
                    "taiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TaiRange"
                        },
                        "minItems": 1
                    },
                    "backupInfoAmfFailure": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Guami"
                        },
                        "minItems": 1
                    },
                    "backupInfoAmfRemoval": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Guami"
                        },
                        "minItems": 1
                    },
                    "n2InterfaceAmfInfo": {
                        "$ref": "#/components/schemas/N2InterfaceAmfInfo"
                    }
                }
            },
            "Guami": {
                "type": "object",
                "additionalProperties": false,
                "required": ["plmnId", "amfId"],
                "properties": {
                    "plmnId": {
                        "$ref": "#/components/schemas/PlmnId"
                    },
                    "amfId": {
                        "$ref": "#/components/schemas/AmfId"
                    }
                }
            },
            "AmfId": {
                "type": "string",
                "pattern": "^[A-Fa-f0-9]{6}$"
            },
            "SmfInfo": {
                "type": "object",
                "additionalProperties": false,
                "required": ["sNssaiSmfInfoList"],
                "properties": {
                    "sNssaiSmfInfoList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SnssaiSmfInfoItem"
                        },
                        "minItems": 1
                    },
                    "taiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Tai"
                        },
                        "minItems": 1
                    },
                    "taiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TaiRange"
                        },
                        "minItems": 1
                    },
                    "pgwFqdn": {
                        "$ref": "#/components/schemas/Fqdn"
                    },
                    "accessType": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/AccessType"
                        },
                        "minItems": 1,
                        "maxItems": 2
                    }
                }
            },
            "SnssaiSmfInfoItem": {
                "type": "object",
                "additionalProperties": false,
                "required": ["sNssai", "dnnSmfInfoList"],
                "properties": {
                    "sNssai": {
                        "$ref": "#/components/schemas/Snssai"
                    },
                    "dnnSmfInfoList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DnnSmfInfoItem"
                        },
                        "minItems": 1
                    }
                }
            },
            "DnnSmfInfoItem": {
                "type": "object",
                "additionalProperties": false,
                "required": ["dnn"],
                "properties": {
                    "dnn": {
                        "$ref": "#/components/schemas/Dnn"
                    }
                }
            },
            "Tai": {
                "type": "object",
                "additionalProperties": false,
                "required": ["plmnId", "tac"],
                "properties": {
                    "plmnId": {
                        "$ref": "#/components/schemas/PlmnId"
                    },
                    "tac": {
                        "$ref": "#/components/schemas/Tac"
                    }
                }
            },
            "Tac": {
                "type": "string",
                "pattern": "(^[A-Fa-f0-9]{4}$)|(^[A-Fa-f0-9]{6}$)"
            },
            "UpfInfo": {
                "type": "object",
                "additionalProperties": false,
                "required": ["sNssaiUpfInfoList"],
                "properties": {
                    "sNssaiUpfInfoList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SnssaiUpfInfoItem"
                        },
                        "minItems": 1
                    },
                    "smfServingArea": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "minItems": 1
                    },
                    "interfaceUpfInfoList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/InterfaceUpfInfoItem"
                        },
                        "minItems": 1
                    },
                    "iwkEpsInd": {
                        "type": "boolean"
                    }
                }
            },
            "SnssaiUpfInfoItem": {
                "type": "object",
                "additionalProperties": false,
                "required": ["sNssai", "dnnUpfInfoList"],
                "properties": {
                    "sNssai": {
                        "$ref": "#/components/schemas/Snssai"
                    },
                    "dnnUpfInfoList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DnnUpfInfoItem"
                        },
                        "minItems": 1
                    }
                }
            },
            "DnnUpfInfoItem": {
                "type": "object",
                "additionalProperties": false,
                "required": ["dnn"],
                "properties": {
                    "dnn": {
                        "$ref": "#/components/schemas/Dnn"
                    },
                    "dnaiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Dnai"
                        },
                        "minItems": 1
                    }
                }
            },
            "InterfaceUpfInfoItem": {
                "type": "object",
                "additionalProperties": false,
                "required": ["interfaceType"],
                "properties": {
                    "interfaceType": {
                        "$ref": "#/components/schemas/UPInterfaceType"
                    },
                    "ipv4EndpointAddresses": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Ipv4Addr"
                        },
                        "minItems": 1
                    },
                    "ipv6EndpointAddresses": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Ipv6Addr"
                        },
                        "minItems": 1
                    },
                    "endpointFqdn": {
                        "$ref": "#/components/schemas/Fqdn"
                    },
                    "networkInstance": {
                        "type": "string"
                    }
                }
            },
            "UPInterfaceType": {
                "anyOf": [{
                        "type": "string",
                        "enum": ["N3", "N6", "N9"]
                    },
                    {
                        "type": "string"
                    }
                ]
            },
            "PcfInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "dnnList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Dnn"
                        },
                        "minItems": 1
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        },
                        "minItems": 1
                    },
                    "rxDiamHost": {
                        "$ref": "#/components/schemas/DiameterIdentity"
                    },
                    "rxDiamRealm": {
                        "$ref": "#/components/schemas/DiameterIdentity"
                    }
                }
            },
            "DiameterIdentity": {
                "type": "string",
                "pattern": "^([A-Za-z0-9]+(-[A-Za-z0-9]+).)+[a-z]{2,}$"
            },
            "BsfInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "dnnList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Dnn"
                        },
                        "minItems": 1
                    },
                    "ipDomainList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "minItems": 1
                    },
                    "ipv4AddressRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Ipv4AddressRange"
                        },
                        "minItems": 1
                    },
                    "ipv6PrefixRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Ipv6PrefixRange"
                        },
                        "minItems": 1
                    }
                }
            },
            "ChfInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "supiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        },
                        "minItems": 1
                    },
                    "gpsiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        },
                        "minItems": 1
                    },
                    "plmnRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/PlmnRange"
                        },
                        "minItems": 1
                    }
                }
            },
            "NrfInfo": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "amfInfoSum": {
                        "$ref": "#/components/schemas/AmfInfoSum"
                    },
                    "smfInfoSum": {
                        "$ref": "#/components/schemas/SmfInfoSum"
                    },
                    "udmInfoSum": {
                        "$ref": "#/components/schemas/UdmInfoSum"
                    },
                    "ausfInfoSum": {
                        "$ref": "#/components/schemas/AusfInfoSum"
                    },
                    "pcfInfoSum": {
                        "$ref": "#/components/schemas/PcfInfoSum"
                    }
                }
            },
            "AmfInfoSum": {
                "type": "object",
                "additionalProperties": false,
                "required": ["amfSetIdList", "amfRegionIdList", "guamiList"],
                "properties": {
                    "amfSetIdList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "amfRegionIdList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "guamiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Guami"
                        }
                    },
                    "taiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Tai"
                        }
                    },
                    "taiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TaiRange"
                        }
                    }
                }
            },
            "SmfInfoSum": {
                "type": "object",
                "additionalProperties": false,
                "required": ["dnnList"],
                "properties": {
                    "dnnList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Dnn"
                        }
                    },
                    "taiList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Tai"
                        }
                    },
                    "taiRangeList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TaiRange"
                        }
                    },
                    "pgwFqdnList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Fqdn"
                        }
                    }
                }
            },
            "UdmInfoSum": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "groupIdList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        }
                    },
                    "gpsiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        }
                    },
                    "externalGroupIdentityfiersRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IdentityRange"
                        }
                    },
                    "routingIndicatorList": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^[0-9]{1,4}$"
                        }
                    }
                }
            },
            "AusfInfoSum": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "groupIdList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        }
                    },
                    "routingIndicatorList": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^[0-9]{1,4}$"
                        }
                    }
                }
            },
            "PcfInfoSum": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "dnnList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Dnn"
                        }
                    },
                    "supiRanges": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SupiRange"
                        }
                    }
                }
            },
