<?xml version='1.0' encoding='UTF-8'?>
<!-- ericsson-nrf-pm-job-instances 0.1.2 -->
<!-- modified: 2019-04-17 -->
<config xmlns="http://tail-f.com/ns/config/1.0">
  <pm xmlns="urn:rdns:com:ericsson:oammodel:ericsson-pm">
    <job>
      <name>eric-nrf-job</name>
      <type>measurement-job</type>
	  <reporting-period>five-min</reporting-period>
      <granularity-period>five-min</granularity-period>
      <current-job-state>stopped</current-job-state>
	  <control>start-stop</control>
      <compression-type xmlns:typese="urn:rdns:com:ericsson:oammodel:ericsson-yang-types">typese:gzip</compression-type>
      <measurement-reader>
        <name>eric-nrf-nnrf-nfm-job</name>
        <group-ref>eric-nrf-nnrf-nfm-group</group-ref>
      </measurement-reader>
	  <measurement-reader>
        <name>eric-nrf-nnrf-disc-job</name>
        <group-ref>eric-nrf-nnrf-disc-group</group-ref>
      </measurement-reader>
	  <measurement-reader>
        <name>eric-nrf-nnrf-prov-job</name>
        <group-ref>eric-nrf-nnrf-prov-group</group-ref>
      </measurement-reader>
    </job>
  </pm>
</config>
