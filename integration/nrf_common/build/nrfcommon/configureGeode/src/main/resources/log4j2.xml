<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
    <Properties>
        <Property name="NETWORK_FUNCTION">NRF</Property>
        <Property name="SERVICE_ID">eric-nrf-configuregeode</Property>
    </Properties>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout>
                <pattern>
                    {"version":"0.2.0","timestamp":"%d{yyyy-MM-dd'T'HH:mm:ssZZ}","severity":"%level{TRACE=debug,DEBUG=debug,INFO=info,WARN=warning,ERROR=error,FATAL=critical}","service_id":"${SERVICE_ID}","metadata":{"function":"${NETWORK_FUNCTION}","ul_id":"%F:%L"},"message":"%m"}%n
                </pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>